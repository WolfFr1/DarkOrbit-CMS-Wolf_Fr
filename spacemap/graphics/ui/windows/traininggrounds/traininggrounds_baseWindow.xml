                        registeredTextStyleName="traininggrounds_whiteTextStyleButton"/>
                <coreponent name="horizontalDivider" marginTop="210" autoSizeWidth="true" marginLeft="22"
                            marginRight="20" registeredSkinName="traininggrounds_window_horizontal_divider"/>
                <coreponent name="infoIcon" marginTop="220" marginLeft="10"
                            registeredSkinName="traininggrounds_window_info_icon"/>
                <label name="funnyDescription" alignX="left" marginTop="240" autoSizeWidth="true" useHTML="true"
                       height="100" localizedText="label_traininggrounds_general_description"
                       registeredSkinName="traininggrounds_label"
                       registeredTextStyleName="traininggrounds_smallDescriptionTextStyle"/>
            </container>

            <container name="tab_stats" y="70" width="320" height="240" alignX="center">
                <label name="left" alignX="left" localizedText="label_TAS_Season"
                       registeredSkinName="traininggrounds_label" width="200"
                       registeredTextStyleName="traininggrounds_tabel_headline"/>
                <label name="right" x="200" localizedText="label_TAS_Rank"
                       registeredSkinName="traininggrounds_label" width="100"
                       registeredTextStyleName="traininggrounds_tabel_headline"/>

                <scrollcontainer name="container"
                                 marginTop="20"
                                 autoSizeWidth="true"
                                 height="208"
                                 marginLeft="5"
                                 marginRight="5"
                                 mouseEnabled="true"
                                 registeredSkinName="tas_window_scrollcontainerskin">
                    <itemlist layout="vertical" width="320" name="content" autoSizeHeight="true"
                              autoSizeMode="children">
                        <itemrenderer height="20">
                            <coreponent name="bg" registeredSkinName="traininggrounds_list" height="20" width="320"/>
                            <label name="left" alignX="left" registeredSkinName="traininggrounds_label" width="200" height="20"
                                   registeredTextStyleName="traininggrounds_tabel_content"/>
                            <label name="right" x="200" height="20" text="0" width="116"
                                   registeredSkinName="traininggrounds_label"
                                   registeredTextStyleName="traininggrounds_tabel_content"/>
                        </itemrenderer>
                    </itemlist>
                </scrollcontainer>
                <scrollbar
                        name="scrollbar"
                        width="10"
                        autoSizeHeight="true"
                        alignX="right"
                        marginRight="6"
                        marginBottom="12"
                        marginTop="20"
                        registeredSkinName="commonui_scrollbar_bgnd">
                    <button
                            name="buttonSlider"
                            width="6"
                            height="6"
                            alignX="center"
                            y="0"
                            registeredSkinName="commonui_scrollbar_slider"/>
                </scrollbar>
                <coreponent registeredSkinName="traininggrounds_list_vertical" x="200" height="226" marginTop="2"
                            width="1"/>
                <coreponent registeredSkinName="TAS_tabel_frame" height="240" marginTop="-5" marginBottom="0"
                            width="320"/>
            </container>
            <container name="tab_rewards" y="70" width="320" height="220" alignX="center">
                <label name="info" width="320"  localizedText="label_TAS_no_season_ranking"
                       visible="false"
                       y="100"
                       registeredSkinName="traininggrounds_label"
                       registeredTextStyleName="traininggrounds_whiteTextStyle" alignX="center"/>
                <container name="content" width="320" height="220">
                    <label name="tabheadline" width="320" alignX="center" localizedText="label_TAS_Rewards"
                           registeredSkinName="traininggrounds_label"
                           registeredTextStyleName="traininggrounds_tabel_headline"/>

                    <scrollcontainer name="container"
                                     marginTop="20"
                                     autoSizeWidth="true"
                                     height="208"
                                     marginLeft="5"
                                     marginRight="5"
                                     mouseEnabled="true"
                                     registeredSkinName="tas_window_scrollcontainerskin">
                        <itemlist layout="vertical" width="320" name="content" height="208" autoSizeHeight="true">
                            <itemrenderer height="20" mouseEnabled="true">
                                <coreponent name="bg" registeredSkinName="traininggrounds_list" height="20" width="320"/>
                                <label name="left" alignX="left" text="" registeredSkinName="traininggrounds_label"
                                       width="270" height="20"
                                       registeredTextStyleName="traininggrounds_tabel_content"/>
                                <coreponent name="infoIcon" x="280"
                                            registeredSkinName="traininggrounds_window_info_icon"/>
                            </itemrenderer>
                        </itemlist>
                    </scrollcontainer>
                    <scrollbar
                            name="scrollbar"
                            width="10"
                            autoSizeHeight="true"
                            alignX="right"
                            marginRight="6"
                            marginBottom="12"
                            marginTop="20"
                            registeredSkinName="commonui_scrollbar_bgnd">
                        <button
                                name="buttonSlider"
                                width="6"
                                height="6"
                                alignX="center"
                                y="0"
                                registeredSkinName="commonui_scrollbar_slider"/>
                    </scrollbar>
                    <coreponent registeredSkinName="TAS_tabel_frame" height="240" marginTop="-5" marginBottom="0"
                                width="320"/>
                </container>
            </container>
        </container>
        <container
