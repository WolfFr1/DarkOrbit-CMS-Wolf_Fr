    <skins>
        <skin
                name="tas_window_scrollcontainerskin"
                classname="ContainerSkin">
            <layer
                    classname="FilledBoxLayer"
                    fillColor="0x358BAC"
                    fillAlpha="1">mask
            </layer>
        </skin>
        <skin name="tas_list_skin" library="gameclientCommonUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="debug_mask_rounded_edges_3x3" scale9grid="3,3,4,4">mask</layer>
        </skin>
        <skin name="traininggrounds_tab_button_left" classname="FadableButtonSkin" library="traininggroundsUI">
            <layer classname="BitmapLayer" libraryID="tab1_normal" scale9grid="17,4,84,13">normal</layer>
            <layer classname="BitmapLayer" libraryID="tab1_hover" scale9grid="17,4,84,13">over</layer>
            <layer classname="BitmapLayer" libraryID="tab1_active" scale9grid="17,4,84,13">clicked</layer>
        </skin>
        <skin name="traininggrounds_tab_button" classname="FadableButtonSkin" library="traininggroundsUI">
            <layer classname="BitmapLayer" libraryID="tab2_normal" scale9grid="3,3,71,13">normal</layer>
            <layer classname="BitmapLayer" libraryID="tab2_hover" scale9grid="3,3,71,13">over</layer>
            <layer classname="BitmapLayer" libraryID="tab2_active" scale9grid="3,3,71,13">clicked</layer>
        </skin>
        <skin name="contactlist_list_skin" library="gameclientCommonUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="debug_mask_rounded_edges_3x3" scale9grid="3,3,4,4">mask</layer>
        </skin>

        <skin name="traininggrounds_label" classname="LabelSkin" library="gameclientCommonUI"/>
        <skin name="traininggrounds_label_button_green" classname="FadableButtonSkin" library="traininggroundsUI">
            <layer classname="BitmapLayer" libraryID="btn_green_normal" scale9grid="22,24,140,40">normal</layer>
            <layer classname="BitmapLayer" libraryID="btn_green_hover" scale9grid="22,24,140,40">over</layer>
            <layer classname="BitmapLayer" libraryID="btn_inactive" scale9grid="22,24,140,40">disabled</layer>
        </skin>
        <skin name="traininggrounds_label_button_red" classname="FadableButtonSkin" library="traininggroundsUI">
            <layer classname="BitmapLayer" libraryID="btn_red_normal" scale9grid="22,24,140,40">normal</layer>
            <layer classname="BitmapLayer" libraryID="btn_red_hover" scale9grid="22,24,140,40">over</layer>
            <layer classname="BitmapLayer" libraryID="btn_inactive" scale9grid="22,24,140,40">disabled</layer>
        </skin>
        <skin name="traininggrounds_label_button_blue" classname="FadableButtonSkin" library="traininggroundsUI">
            <layer classname="BitmapLayer" libraryID="btn_blue_normal" scale9grid="22,24,140,40">normal</layer>
            <layer classname="BitmapLayer" libraryID="btn_blue_hover" scale9grid="22,24,140,40">over</layer>
            <layer classname="BitmapLayer" libraryID="btn_inactive" scale9grid="22,24,140,40">disabled</layer>
        </skin>
        <skin name="traininggrounds_window_horizontal_divider" library="gameclientCommonUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="horizontal_divider">normal</layer>
        </skin>
        <skin name="traininggrounds_window_info_icon" library="traininggroundsUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="info_icon">normal</layer>
        </skin>
        <skin name="traininggrounds_window_heroPicFrame" library="traininggroundsUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="pic_frame_player">normal</layer>
        </skin>
        <skin name="traininggrounds_window_oppoPicFrame" library="traininggroundsUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="pic_frame_opponent">normal</layer>
        </skin>
        <skin name="traininggrounds_window_barsAndRoundBg" library="traininggroundsUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="barsAndRoundBg">normal</layer>
        </skin>
        <skin name="traininggrounds_window_greenBar" library="traininggroundsUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="greenBar">normal</layer>
        </skin>
        <skin name="traininggrounds_window_blueBar" library="traininggroundsUI" classname="BasicSkin">
            <layer classname="BitmapLayer" libraryID="blueBar">normal</layer>
        </skin>
        <skin name="traininggrounds_victorypoint_checkbox9x8" library="traininggroundsUI" classname="CheckBoxSkin">
            <layer classname="BitmapLayer" libraryID="point_on">normal.selected</layer>
            <layer classname="BitmapLayer" libraryID="point_off">normal</layer>
            <layer classname="BitmapLayer" libraryID="point_on">over.selected</layer>
            <layer classname="BitmapLayer" libraryID="point_off">over</layer>
        </skin>
        <skin name="TAS_ttip_frame" library="traininggroundsUI" classname="BasicSkin">
            <layer classname="BitmapLayer" scale9grid="1,1,2,2" libraryID="ttip_frame">normal</layer>
        </skin>
        <skin name="traininggrounds_list" classname="BasicSkin">
            <layer classname="FilledBoxLayer" fillColor="0x141c22"
                   fillAlpha="1">normal
            </layer>
        </skin>

        <skin name="traininggrounds_list_vertical" classname="BasicSkin">
            <layer classname="FilledBoxLayer" fillColor="0x444444"
                   fillAlpha="1">normal
            </layer>
        </skin>

        <skin name="TAS_tabel_frame" library="traininggroundsUI" classname="BasicSkin">
            <layer classname="BitmapLayer" scale9grid="11,24,13,25" libraryID="table_bg_scale9">normal</layer>
        </skin>
        <skin name="traininggrounds_window_rank_indicator" library="traininggroundsUI" classname="BasicStateSkin">
            <layer classname="BitmapLayer" libraryID="arrow_up">up</layer>
            <layer classname="BitmapLayer" libraryID="arrow_down">down</layer>
            <layer classname="FilledBoxLayer" fillAlpha="0">none</layer>
        </skin>

    </skins>

    <container
            name="windowContentContainer"
            width="320"
            height="400"
            x="10"
            mouseEnabled="false"
            >
        <container
                name="tgDisabledStageContainer"
                width="320"
                height="340"
                mouseEnabled="false"
                visible="false"
                >
            <label name="disabledReasonLabel" alignX="center" autoSizeHeight="true" marginTop="191" width="300" text="TA disabled. REPLACE ME"
                   registeredSkinName="traininggrounds_label" registeredTextStyleName="traininggrounds_whiteTextStyle"/>
        </container>
        <container
                name="initialStageContainer"
                width="320"
                height="320"
                mouseEnabled="false"
                visible="true"
                >
            <container name="tabbar" layout="horizontal" y="25" width="320" gap="1">
                <button name="match" width="*" height="15" localizedText="button_tas_match"
                        registeredSkinName="traininggrounds_tab_button_left"
                        registeredTextStyleName="traininggrounds_tabButton"/>
                <button name="stats" width="*" height="15" localizedText="button_tas_stats"
                        registeredSkinName="traininggrounds_tab_button"
                        registeredTextStyleName="traininggrounds_tabButton"/>
                <button name="rewards" width="*" height="15" localizedText="button_tas_rewards"
                        registeredSkinName="traininggrounds_tab_button"
                        registeredTextStyleName="traininggrounds_tabButton"/>
            </container>
            <label name="seasonLabel" alignX="left" marginTop="50" autoSizeWidth="true" height="20"
                   localizedText="label_traininggrounds_season" registeredSkinName="traininggrounds_label"
                   registeredTextStyleName="traininggrounds_smallDescriptionTextStyle_yellow"/>
            <container name="tab_match" width="320">
                <label name="yourVictoriesLabel" alignX="center" marginTop="71" autoSizeWidth="true" height="20" text=""
                       registeredSkinName="traininggrounds_label"
                       registeredTextStyleName="traininggrounds_whiteTextStyle"/>
                <label name="battlesPlayedLabel" alignX="center" marginTop="91" autoSizeWidth="true" height="20" text=""
                       registeredSkinName="traininggrounds_label"
                       registeredTextStyleName="traininggrounds_whiteTextStyle"/>
                <label name="scoreLabel" alignX="center" marginTop="111" autoSizeWidth="true" height="20" text=""
                       registeredSkinName="traininggrounds_label"
                       registeredTextStyleName="traininggrounds_whiteTextStyle"/>
                <container name="rankContainer" height="20" width="320"
                           marginTop="131" alignX="center" layout="horizontal">
                    <label name="rankLabel" height="20" text="" width="160"
                           registeredSkinName="traininggrounds_label" x="160"
                           registeredTextStyleName="traininggrounds_whiteTextStyle_align_right"/>
                    <coreponent name="rankIcon" marginTop="7"
                                registeredSkinName="traininggrounds_window_rank_indicator" alignX="center"
                            />
                    <label name="rankScore" height="20" text="0"
                           x="160"
                           width="160"
                           registeredSkinName="traininggrounds_label"
                           registeredTextStyleName="traininggrounds_whiteTextStyle_align_left"/>
                </container>
                <button name="confirmationButton" width="220" height="62" marginTop="141" alignX="center"
