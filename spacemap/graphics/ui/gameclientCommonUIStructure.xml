			<root>
				<button name="decreaseButton" width="16" height="16" registeredSkinName="commonui_spinbox_button_symbol_minus" />
				<input  name="valueInput"     width="*"  height="17" registeredSkinName="commonui_spinbox_background" registeredTextStyleName="commonui_spinbox_input_textstyle" />
