			<layer classname="BitmapLayer" libraryID="tabbar74x15_over" scale9grid="2,2,71,13">over</layer>
			<layer classname="BitmapLayer" libraryID="tabbar74x15_selected" scale9grid="2,2,71,13" >clicked</layer>
