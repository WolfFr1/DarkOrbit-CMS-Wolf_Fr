			<layer classname="BitmapLayer" libraryID="input_background_invalid" scale9grid="19,8,21,31" >normal</layer>
		</skin>
		<template name="commonui_input_invalid_popup" >
			<root>
				<container name="popup" width="268" height="40" layout = "vertical" autoHeightMode="children" registeredSkinName="commonui_input_invalid" >
					<coreponent height="10" />
					<label  name="message" text="" marginLeft="18"  marginRight="8" autoSizeWidth="true" autoSizeTextHeight="true" registeredTextStyleName="textstyle_align_left_eurostilefl_12" />
					<coreponent height="10" />
				</container>
			</root>
		</template>
		<textstyle  name               = "commonui_input_textstyle"
					textColor          = "0xFEFEFE"
					borderColor        = "0x555555"
					backgroundColor    = "0x000000"
					selectionTextColor = "0x333344"
					type               = "input"
					selectable         = "true"
					marginLeft         = "5"
					fontName           = "Arial"
					embedFonts         = "false"
					border             = "false"
					background         = "false"
					maxChars           = "128"
				/>

