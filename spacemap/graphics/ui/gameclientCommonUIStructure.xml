		<skin name="commonui_tabbarbutton" library="gameclientCommonUI" classname="FadableButtonSkin" >
			<layer classname="BitmapLayer" libraryID="tabbar74x15_normal"   scale9grid="2,2,71,13" >normal</layer>
			<layer classname="BitmapLayer" libraryID="tabbar74x15_over"     scale9grid="2,2,71,13" >over</layer>
			<layer classname="BitmapLayer" libraryID="tabbar74x15_normal"   scale9grid="2,2,71,13" >clicked</layer>
		</skin>

